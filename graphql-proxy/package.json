    "graphql": "^14.5.8",
    "ts-node": "^8.6.1",
    "typescript": "^3.7.4"
  },
  "devDependencies": {
    "@types/cookie": "0.3.3",
    "@types/cookie-parser": "^1.4.2",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1"
  }
}
